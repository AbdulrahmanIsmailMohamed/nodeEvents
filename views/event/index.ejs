<%- include('../partials/header') -%>
<%- include('../partials/navbar') -%>
<div class="container">
  <div class="row mt-3">
    <div class="col md-12">
      <%- include('../partials/flashMessages') -%>
    </div>
  </div>
  <% chunk.forEach((events)=> {%>
  <div class="row mt-3">
    <% events.forEach((events)=> {%>
    <div class="col-sm-6 col-md-4">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">
            <%= events.title%>
          </h5>
          <p class="card-text">
            <%= events.description%>
          </p>
          <p>
            <%= events.location%>
          </p>
          <a href="<%= "/events/getevent/" + events.id%>" class="btn btn-primary">Show More</a>
        </div>
      </div>
    </div>
    <%})%>
  </div>
  <%})%>
</div>
<%- include('../partials/footer') -%>